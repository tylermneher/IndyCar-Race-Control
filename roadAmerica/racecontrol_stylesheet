    <script>
        $(document).ready(function () {
            $(window).scroll(function () {
                yos = (window.pageYOffset > $("#hiddenOffset").val());
                var nta = $("#hiddenNoTrackActivity").val();
                if (yos && nta == 'FALSE') {
                    $("#StickyVideoBar").addClass("stickyVideo");
                    $("#StickyVideoBar").removeClass("hiddenStickyVideo");
                    $("#VideoDiv").addClass("scrolledTop");
                    $("#VideoDiv").removeClass("nonScrolled");
                    $("#SessionInformationDiv").addClass("sessionInformationDivScrolled");
                    $("#SessionInformationDiv").removeClass("sessionInformationNonScrolled");
                    if ($("#hiddenSeries").val() == 'I' || $("#hiddenSeries").val() == 'L') {
                        $("#FeatureDriverDiv").addClass("featuredDriverScrolled");
                        $("#FeatureDriverDiv").removeClass("featuredDriverHidden");
                    }
                    else {
                        $("#FeatureDriverDiv").removeClass("featuredDriverScrolled");
                        $("#FeatureDriverDiv").addClass("featuredDriverHidden");
                    }
                    $("#FlagDiv").addClass("flagScrolled");
                    $("#FlagDiv").removeClass("flagNonScrolled");
                    $("#SessionInfoDiv").removeClass("SessionInfoDivNonScrolled");
                    $("#SessionInfoDiv").addClass("SessionInfoDivScrolled");

                } else {
                    $("#StickyVideoBar").addClass("hiddenStickyVideo");
                    $("#StickyVideoBar").removeClass("stickyVideo");
                    $("#VideoDiv").addClass("nonScrolled");
                    $("#VideoDiv").removeClass("scrolledTop");
                    $("#SessionInformationDiv").removeClass("sessionInformationDivScrolled");
                    $("#SessionInformationDiv").addClass("sessionInformationNonScrolled");
                    if ($("#hiddenSeries").val() == 'I' || $("#hiddenSeries").val() == 'L') {
                        $("#FeatureDriverDiv").removeClass("featuredDriverScrolled");
                        $("#FeatureDriverDiv").addClass("featuredDriverHidden");
                    }
                    $("#FlagDiv").removeClass("flagScrolled");
                    $("#FlagDiv").addClass("flagNonScrolled");
                    $("#SessionInfoDiv").addClass("SessionInfoDivNonScrolled");
                    $("#SessionInfoDiv").removeClass("SessionInfoDivScrolled");
                }
            });
        });
    </script>

